{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup src/index.ts --format cjs",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format cjs --watch --onSuccess \"node dist/index.js\"",
    "lint": "tsc --noEmit && TIMING=1 eslint \"src/**/*.ts*\"",
    "start": "node dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "got": "^11.8.3",
    "logger": "*",
    "luxon": "^3.1.0",
    "morgan": "^1.10.0",
    "query-string": "^7.1.1",
    "shared-types": "*",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/got": "^9.6.12",
    "@types/jest": "^26.0.22",
    "@types/luxon": "^3.0.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "supertest": "^6.2.4",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
